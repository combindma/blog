{
    "name": "combindma/blog",
    "description": "Blog",
    "keywords": [
        "combindma",
        "laravel",
        "blog"
    ],
    "homepage": "https://github.com/combindma/blog",
    "license": "MIT",
    "authors": [
        {
            "name": "Combind",
            "email": "hello@combind.ma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Combindma\\Blog\\": "src",
            "Combindma\\Blog\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Combindma\\Blog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Combindma\\Blog\\BlogServiceProvider"
            ],
            "aliases": {
                "Blog": "Combindma\\Blog\\BlogFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
