{
    "name": "combindma/blog",
    "description": "Blog",
    "keywords": [
        "combind",
        "laravel",
        "blog"
    ],
    "homepage": "https://github.com/combindma/blog",
    "license": "MIT",
    "authors": [
        {
            "name": "Combind",
            "email": "hello@combind.ma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "bensampo/laravel-enum": "^3.3",
        "combindma/dashui": "dev-master",
        "combindma/flash": "^1.0",
        "cviebrock/eloquent-sluggable": "^8.0",
        "elegantweb/sanitizer": "^1.0",
        "illuminate/contracts": "^8.0",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-medialibrary": "^9.5",
        "spatie/laravel-package-tools": "^1.4.3",
        "tucker-eric/eloquentfilter": "^2.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Combindma\\Blog\\": "src",
            "Combindma\\Blog\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Combindma\\Blog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-filter": "vendor/bin/phpunit --no-coverage --filter",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Combindma\\Blog\\BlogServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/combindma/dashui"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
